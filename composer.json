{
  "name": "nanigans/single-table-inheritance",
  "description": "Single Table Inheritance Trait",
  "keywords": ["single", "table", "inheritance", "laravel", "eloquent", "model"],
  "license": "MIT",
  "authors": [
    {
      "name": "Jon Palmer",
      "email": "jpalmer@nanigans.com"
    }
  ],
  "require": {
    "php": ">= 7.2",
    "illuminate/database": ">= 5.2",
    "illuminate/support": ">= 5.2"
  },
  "require-dev": {
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~8.0|~9.0",
    "orchestra/testbench": "3.8.*|4.*|5.*|6.*",
    "orchestra/database": "3.8.*|4.*|5.*|6.*"
  },
  "autoload": {
    "psr-4": {
      "Nanigans\\SingleTableInheritance\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nanigans\\SingleTableInheritance\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
        "dev-master": "0.9.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
